import sys
import os

try:
    from pclean import PCleanApplication
except ImportError:
    sys.path.append(os.path.join(os.path.dirname(__file__), ".."))
    from pclean import PCleanApplication

def main():
    application = PCleanApplication()
    application.run()

if __name__ == "__main__":
    main()

